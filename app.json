{
  "expo": {
    "name": "LanguageSpeedTrain",
    "slug": "LanguageSpeedTrain",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#938562"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elasting.LangSpeedTrain"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpg",
        "backgroundColor": "#938562"
      },
      "package": "com.elasting.LangSpeedTrain"
    },
    "androidStatusBar": {
      "backgroundColor": "#6200ee",
      "translucent": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
